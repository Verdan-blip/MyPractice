<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="com.example.summerpractice.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main" >
        <action
            android:id="@+id/action_mainFragment_to_premiumStatusFragment"
            app:destination="@id/premiumStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/clipsFragment"
        android:name="com.example.summerpractice.ClipsFragment"
        android:label="fragment_clips"
        tools:layout="@layout/fragment_clips" >
        <action
            android:id="@+id/action_clipsFragment_to_premiumStatusFragment"
            app:destination="@id/premiumStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/messengerFragment"
        android:name="com.example.summerpractice.MessengerFragment"
        android:label="fragment_messenger"
        tools:layout="@layout/fragment_messenger" >
        <action
            android:id="@+id/action_messengerFragment_to_premiumStatusFragment"
            app:destination="@id/premiumStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/premiumStatusFragment"
        android:name="com.example.summerpractice.PremiumStatusFragment"
        android:label="fragment_premium_status"
        tools:layout="@layout/fragment_premium_status" />
    <fragment
        android:id="@+id/servicesFragment"
        android:name="com.example.summerpractice.ServicesFragment"
        android:label="fragment_services"
        tools:layout="@layout/fragment_services" >
        <action
            android:id="@+id/action_servicesFragment_to_premiumStatusFragment"
            app:destination="@id/premiumStatusFragment" />
    </fragment>
    <fragment
        android:id="@+id/videosFragment"
        android:name="com.example.summerpractice.VideosFragment"
        android:label="fragment_videos"
        tools:layout="@layout/fragment_videos" >
        <action
            android:id="@+id/action_videosFragment_to_premiumStatusFragment"
            app:destination="@id/premiumStatusFragment" />
    </fragment>
</navigation>